    "koa-session": "^5.8.1",
    "sha1": "^1.1.1",
    "vue": "2.5.16",
    "vue-meta": "^1.4.3",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.16",
    "vuex": "^3.0.1"
