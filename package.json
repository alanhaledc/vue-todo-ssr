{
  "name": "vue-todo-ssr",
  "version": "2.0.0",
  "description": "update to webpack4 and babel7",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "clean": "rimraf client-dist && rimraf server-dist",
    "upload": "node build/upload.js",
    "lint": "eslint --ext .js --ext .jsx --ext .vue client/",
    "lint-fix": "eslint --fix --ext .js --ext .jsx --ext .vue client/",
    "precommit": "npm run lint-fix",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.client.config.js",
    "dev:server": "nodemon server/app.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "start": "cross-env NODE_ENV=production node server/app.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "ejs": "^2.5.7",
